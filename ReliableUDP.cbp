<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ReliableUDP" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="server">
				<Option output="bin/server" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/server/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add directory="include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-lpthread" />
				</Linker>
			</Target>
			<Target title="client">
				<Option output="bin/client" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/client/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add directory="include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="all" targets="server;client;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
		</Compiler>
		<Unit filename="client.cpp">
			<Option target="client" />
		</Unit>
		<Unit filename="server.cpp">
			<Option target="server" />
		</Unit>
		<Unit filename="udp-util.cpp" />
		<Unit filename="udp-util.h" />
		<Extensions>
			<envvars />
			<code_completion />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
